
<launch>

	<machine name="local_alt" address="localhost" default="true" />

	<arg name="ros_config_path" default="$(optenv ROS_CONFIG_PATH)"/>

	<remap from="/husky_hwu/camera/image" to="/stereo_camera/left/image_raw"/>
	<remap from="/husky_hwu/odom" to="/odometry/filtered"/>
	
	<node name="RatSLAMLocalViewCells" pkg="ratslam_ros" type="ratslam_lv" args="$(arg ros_config_path)/ratslam/ratslam_robotarium.txt _image_transport:=raw" output="screen" cwd="node" required="true" />
	<node name="RatSLAMPoseCells" pkg="ratslam_ros" type="ratslam_pc" args="$(arg ros_config_path)/ratslam/ratslam_robotarium.txt _image_transport:=raw" output="screen" cwd="node" required="true" />
	<node name="RatSLAMExperienceMap" pkg="ratslam_ros" type="ratslam_em" args="$(arg ros_config_path)/ratslam/ratslam_robotarium.txt _image_transport:=raw" output="screen" cwd="node" required="true" />

	<!-- <node pkg="rqt_plot" type="rqt_plot" name="plot_vt_em" args="/irat_red/LocalView/Template/current_id,/irat_red/PoseCell/TopologicalAction/dest_id" /> -->
	<!-- <node pkg="rosbag" type="record" name="record" args="/irat_red/ExperienceMap/Map /irat_red/ExperienceMap/RobotPose /irat_red/LocalView/Template /irat_red/PoseCell/TopologicalAction /overhead/pose -O ratslam_out.bag" /> -->

</launch>
 